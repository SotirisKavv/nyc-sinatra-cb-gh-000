<form method="post" action="/landmarks/<%=@landmark.id%>">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <label for="name">Name </label>
  <input type="text" name="landmark[name]" id="landmark_name" value="@landmark.name">
  <label for="year_completed">Year Completed </label>
  <input type="text" name="landmark[year_completed]" id="landmark_year_completed" value="@landmark.year_completed">
  <input type="submit" value="Create New Landmark">
  <br>
</form>
